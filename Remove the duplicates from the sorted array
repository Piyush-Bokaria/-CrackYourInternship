//Day 2 of #crackYourInternship
class Solution {
    public int removeDuplicates(int[] nums) {
        int n = nums.length - 1;
        int i = 0;
        int j = 1;
        while(j <= n){
            if(nums[i] == nums[j]){
                j++;
                continue;
            }
            nums[i + 1] = nums[j];
            i++;
            j++;
        }
        return ++i;
    }
}
