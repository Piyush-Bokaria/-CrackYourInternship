//Day 1 of #CrackYour Internship
class Solution {
    public void sortColors(int[] nums) {
        int[] arr = new int[3];
        for(int i : nums){
            arr[i]++;
        }
        int i = 0;
        for(i = 0;i< arr[0];i++){
            nums[i] = 0;
        }
        for(;i< arr[0] + arr[1];i++){
            nums[i] = 1;
        }
        for(;i< nums.length;i++){
            nums[i] = 2;
        }
    }
}
